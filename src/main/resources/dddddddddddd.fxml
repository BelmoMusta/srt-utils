<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.WindowController">
    <top>
        <BorderPane>
            <left>
                <Button fx:id="load" mnemonicParsing="false" prefHeight="26.0" prefWidth="73.0" text="load" BorderPane.alignment="CENTER">
                    <BorderPane.margin>
                        <Insets left="10.0" />
                    </BorderPane.margin>
                </Button>
            </left>
            <right>
                <Button fx:id="changeFont" mnemonicParsing="false" prefHeight="26.0" prefWidth="159.0" text="Change Font" BorderPane.alignment="CENTER">
                    <BorderPane.margin>
                        <Insets right="10.0" />
                    </BorderPane.margin>
                </Button>
            </right>
        </BorderPane>
    </top>

        <ScrollPane fx:id="scrollPane" />
    <center>
            <BorderPane fx:id="container" prefHeight="400" prefWidth="400" BorderPane.alignment="CENTER">
            <left>
                <HBox fx:id="leftSide">

                </HBox>
            </left>
            <right>
                <HBox fx:id="rightSide">

                </HBox>

            </right>
         <center>
            <GridPane fx:id="grid" BorderPane.alignment="CENTER">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                </rowConstraints>
            </GridPane>
         </center>
            </BorderPane>
    </center>

</BorderPane>
